<div class="contents row">
  <%= form_for @tweet, urld: tweet_path, method: :patch do |f| %>
    <h3>編集する</h3>
    <%= f.text_field :image, placeholder: "Image", autofocus: true %>
    <%= f.text_area :text, placeholder: "text", rows: "10", col: "30" %>
    <%= f.submit "SENT" %>
  <% end %>
</div>
